package betaben.game;

public class Board {
	
	private String[][] board;
	
	public Board(int row, int col){
		board = new String[row][col];
	}
	
	public String getValue(int row, int col){
		return board[row][col];
	}
	
	public void setValue(int row, int col, String color){
		board[row][col] = color;
	}
	
	
	
}
